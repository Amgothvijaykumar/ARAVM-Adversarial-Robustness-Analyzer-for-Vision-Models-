"""
Load specific images for Captum Insights visualization
"""

import torch
from captum.insights import AttributionVisualizer, Batch
from captum.insights.attr_vis.features import ImageFeature
from torchvision import models, transforms
from PIL import Image
import torch.nn.functional as F

# Load model
model = models.resnet50(pretrained=True)
model.eval()

# Define your specific images
image_paths = [
    "path/to/image1.jpg",
    "path/to/image2.jpg",
    "path/to/image3.jpg",
    "path/to/image4.jpg",
]

# Transform
transform = transforms.Compose([
    transforms.Resize(256),
    transforms.CenterCrop(224),
    transforms.ToTensor()
])

# Normalization
normalize = transforms.Normalize(
    mean=[0.485, 0.456, 0.406],
    std=[0.229, 0.224, 0.225]
)

# Create data iterator from specific images
def specific_image_iterator(paths):
    """Iterator for specific images"""
    images = []
    for path in paths:
        img = Image.open(path).convert('RGB')
        img_tensor = transform(img)
        images.append(img_tensor)
    
    # Yield all as one batch
    yield Batch(
        inputs=torch.stack(images),
        labels=torch.zeros(len(images), dtype=torch.long)
    )

# Get ImageNet classes
def get_imagenet_classes():
    import urllib
    url = "https://raw.githubusercontent.com/pytorch/hub/master/imagenet_classes.txt"
    with urllib.request.urlopen(url) as f:
        return [line.decode('utf-8').strip() for line in f.readlines()]

# Create visualizer
visualizer = AttributionVisualizer(
    models=[model],
    score_func=lambda o: F.softmax(o, 1),
    classes=get_imagenet_classes(),
    features=[
        ImageFeature(
            "Image",
            baseline_transforms=[lambda x: x * 0],
            input_transforms=[normalize],
        )
    ],
    dataset=specific_image_iterator(image_paths),
)

# Render
visualizer.render()